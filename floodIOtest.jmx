            <stringProp name="parameters"></stringProp>
            <stringProp name="script">import org.jsoup.Jsoup;
import org.jsoup.nodes.Document;
import org.jsoup.select.Elements;

import java.util.Arrays;

Document html = Jsoup.parse(prev.getResponseDataAsString());
Elements labels = html.getElementsByTag(&quot;label&quot;);
String[] orderValuesStr = labels.text().split(&quot; &quot;);

int[] orderValues = new int[orderValuesStr.length];
for (int i = 0; i &lt; orderValuesStr.length; i++) {
     orderValues[i] = Integer.parseInt(orderValuesStr[i]);
 }
Arrays.sort(orderValues);
int max = orderValues[orderValues.length - 1];

vars.put(&quot;max&quot;, String.valueOf(max));
</stringProp>
